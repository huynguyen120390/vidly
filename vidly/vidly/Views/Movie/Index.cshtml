@model IEnumerable<vidly.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer</h2>
<table id="customers" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th scope="col">Customer</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var customer in Model)
            {   <tr>
                    <td>@customer.Name</td>
                </tr>
            }
        }
    </tbody>
</table>

